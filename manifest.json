{
    "name": "Unic",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Theme changer and Night light automation",
    "icons": {
        "128": "unic.png"
    },
    "action":{
        "default_icon": "unic.png",
        "default_popup": "popup.html"
    },
   "background":{
       "service_worker": "background.js"
   },
   "options_page":"./options.html",
   "host_permissions":[
        "http://*/*",
        "https://*/*",
        "*://*/*",
        "<all_urls>"
    ],
    "permissions":[
        "activeTab",
        "tabs",
        "scripting"
    ]
    
}
